<configuration>
    <appender
        name="STDOUT"
        class="ch.qos.logback.core.ConsoleAppender" >
        <encoder>
            <pattern>[%method] - %msg%n</pattern>
        </encoder>
    </appender>

    <root level="TRACE" >
        <appender-ref ref="STDOUT" />
    </root>
</configuration>